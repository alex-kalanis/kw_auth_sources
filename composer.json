{
    "name": "alex-kalanis/kw_auth_sources",
    "description": "Authentication sources for KWCMS",
    "keywords": [
        "authentication",
        "auth",
        "ldap",
        "file",
        "files",
        "database"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_accounts": ">=3.0 <4",
        "alex-kalanis/kw_locks": ">=3.0 <4",
        "alex-kalanis/kw_paths": ">=4.0 <5",
        "php": ">=7.4.0"
    },
    "require-dev": {
        "ext-json": "*",
        "alex-kalanis/kw_mapper": ">=3.0 <4",
        "alex-kalanis/kw_storage": ">=5.0 <6",
        "alex-kalanis/kw_files": ">=4.0 <5",
        "shipmonk/composer-dependency-analyser": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=8.0 <=9"
    },
    "suggest": {
        "alex-kalanis/kw_mapper": "Mapper for accessing resources in KWCMS"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_auth_sources\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
